[global_config]
  always_split_with_profile = True
  title_hide_sizetext = True
  title_inactive_bg_color = "#1e1e1e"
  title_inactive_fg_color = "#868686"
  title_transmit_bg_color = "#3b3b3b"
[keybindings]
  go_down = <Alt>j
  go_left = <Alt>h
  go_right = <Alt>l
  go_up = <Alt>k
  layout_launcher = None
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = Gruvbox Dark
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[Gruvbox Dark]]
    background_color = "#1d2021"
    cursor_color = "#bbbbbb"
    font = Hack 9
    foreground_color = "#ebdbb2"
    palette = "#282828:#cc241d:#98971a:#d79921:#458588:#b16286:#689d6a:#a89984:#928374:#fb4934:#b8bb26:#fabd2f:#83a598:#d3869b:#8ec07c:#ebdbb2"
    scrollback_infinite = True
    scrollbar_position = hidden
    show_titlebar = False
    use_system_font = False
